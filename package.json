{
  "name": "sfox",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "uuid": "^13.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec 'node --no-warnings --loader ts-node/esm' src/index.ts",
    "db:seed": "prisma generate && tsx prisma/seed.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "npm run db:seed"
  }
}
